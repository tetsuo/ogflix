  "scripts": {
    "lint": "eslint src/**/* test/**/* --ext .js,.jsx,.ts,.tsx",
    "prettier": "prettier --list-different \"./{src,test}/**/*.ts\"",
    "clean": "rimraf lib public/bundle.js.*",
    "build": "tsc --project tsconfig-server.json",
    "watch": "rimraf lib && tsc --project tsconfig-server.json --watch --sourceMap",
    "webpack": "rimraf public/bundle.js.* && webpack --config webpack.config.js",
    "webpack-stats": "rimraf stats.json && webpack --json --config webpack.config.js > stats.json",
    "release": "npm run clean && npm run build && NODE_ENV=production npm run webpack",
    "jest-clear-cache": "jest --clearCache",
    "jest": "jest --collect-coverage",
    "test": "npm run lint && npm run prettier && npm run jest-clear-cache && npm run jest",
    "start": "node lib/server/index.js"
